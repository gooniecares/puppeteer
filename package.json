{
  "name": "puppetterproject",
  "version": "1.0.0",
  "description": "A software Testing solution using Puppeteer",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha --timeout=60000 ./tests",
    "test:device": "node ./node_modules/mocha/bin/mocha --timeout=60000 ./tests/device.test.js",
    "test:devicejs": "mocha --require ts-node/register --timeout=60000 ./tests/device.test.js",
    "test:devicets": "mocha --require ts-node/register --timeout=60000 ./tests/device.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gooniecares/puppeteer.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gooniecares/puppeteer/issues"
  },
  "homepage": "https://github.com/gooniecares/puppeteer#readme",
  "dependencies": {
    "chai": "^5.1.0",
    "puppeteer": "^22.4.1",
    "request": "^2.88.2",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "dotenv": "^16.4.5",
    "mocha": "^10.3.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2"
  }
}
